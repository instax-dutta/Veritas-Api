{
  "manifest_version": 3,
  "name": "Veritas",
  "version": "1.0",
  "description": "Navigate online safely, Veritas shields you from deceptive phishing schemes.",
  "permissions": ["tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "green16.png",
      "48": "green48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
